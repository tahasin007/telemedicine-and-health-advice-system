
{{> _patientNav}}
<!-- end of navbar -->

<!-- patient table -->
<section>
  <div class="container-fluid" >
    <div class="row" >
      <div class="col-xl-10 col-lg-9 col-md-8 ml-auto mt-4">
        <div class="pt-md-5 mt-md-3 mb-5 col-xl-12" >
          <div class="col-12 rounded"  style="border:3px solid grey;">
            <!-- Search box -->
            <form action="symptompChecker" method="POST" id="booking-form">
              <div class="tab-content">
                <div class="tab-pane" id="step1">
                  <ul>
                    <div class="container">
                      <h3 class="text-info font-weight-bold text-center mb-3">Info</h3>
                    </div>
                    <li>
                      <label>Gender</label><br>
                      <select name="gender" id="gender">
                        <option value="male">Male</option>
                        <option value="female">Female</option>
                      </select>
                    </li>

                    <li>
                      <label>Age</label><br>
                      <input name="age" type="number" min="10" max="100" id="age">
                    </li>
                    <li style="list-style: none; display: inline">
                      <button class="next-btn next-btn1 " type="button">Next</button>
                    </li>
                  </ul>
                </div>

                <div class="tab-pane" id="step2">
                  <div class="container">
                    <h3 class="text-info font-weight-bold text-center mb-3">Enter Your Conditions</h3>
                  </div>
                  <div class="row d-flex justify-content-center">
                    <div class="input-group md-form form-sm form-1 pl-0 col-6  text-center">
                      <input class="form-control my-0 py-1" type="text" name="symptom" placeholder="Search" aria-label="Search">
                    </div>
                  </div>

                  <div class="row justify-content-left" >
                    <div class="containerCB col-8 align-items-left" style="border:3px solid grey;">
                      <ul class="ks-cboxtags">
                        <li><input type="checkbox" id="fever" name="fever" value="fever"><label for="fever">Fever</label></li>
                        <li><input type="checkbox" id="vomiting" name="vomiting" value="vomiting"><label for="vomiting">Vomiting</label></li>
                        <li><input type="checkbox" id="nausea" name="nausea" value="nausea"><label for="nausea">Nausea</label></li>
                        <li><input type="checkbox" id="loss_of_appetite" name="loss_of_appetite" value="loss of appetite"><label for="loss_of_appetite">Loss of Appetite</label></li>
                        <li><input type="checkbox" id="weight_loss" name="weight_loss" value="weight loss"><label for="weight_loss">Weight Loss</label></li>
                        <li><input type="checkbox" id="abdominal_pain" name="abdominal_pain" value="abdominal pain"><label for="abdominal_pain">Abdominal Pain</label></li>
                        <li><input type="checkbox" id="fatigue" name="fatigue" value="fatigue"><label for="fatigue">Fatigue</label></li>
                        <li><input type="checkbox" id="shortness_of_breath" name="shortness_of_breath" value="shortness of breath"><label for="shortness_of_breath">Shortness of Breath</label></li>
                        <li><input type="checkbox" id="sweating" name="sweating" value="sweating"><label for="sweating">Sweating</label></li>
                        <li><input type="checkbox" id="cough" name="cough" value="cough"><label for="cough">Cough</label></li>
                        <li class="ks-selected"><input type="checkbox" id="wheezing" name="wheezing" value="wheezing"><label for="wheezing">Wheezing</label></li>
                        <li><input type="checkbox" id="headache" name="headache" value="headache"><label for="headache">Headache</label></li>
                        <li><input type="checkbox" id="chest_pain" name="chest_pain" value="chest pain"><label for="chest_pain">Chest Pain</label></li>
                        <li><input type="checkbox" id="chills" name="chills" value="chills"><label for="chills">Chills</label></li>
                        <li><input type="checkbox" id="breathing_difficulties" name="breathing_difficulties" value="breathing difficulties"><label for="breathing_difficulties">Breathing Difficulties</label></li>
                      </ul>
                    </div>

                  </div>

                  <li>
                    <button class="next-btn next-btn2 mb-3" id="checker" type="button">Next</button>
                  </li>

                </div>

                <div class="tab-pane" id="step3">
                  <ul>
                    <li>
                      <label>What is your first and last name?</label>
                      <div class="errorTxt"></div>
                      <input type="text" name="bf_fullname">
                    </li>
                    <li>
                      <label>Which email address should we send your free estimate to?</label>
                      <div class="errorTxt"></div>
                      <input type="email" name="bf_email">
                    </li>
                    <li>
                      <label>Do you have any questions or a message? (Optional)</label>
                      <textarea name="bf_message"></textarea>
                    </li>
                    <li>
                      <button class="submit-btn" type="submit">Request My Free Estimate</button>
                    </li>
                  </ul>
                </div>
              </div>

              <div class="progress-wrap">
                <div class="line-progress-bar">
                  <div class="line"></div>
                  <ul class="checkout-bar">
                    <li class="progressbar-dots active"><span>step 1</span></li>
                    <li class="progressbar-dots"><span>step 2</span></li>
                    <li class="progressbar-dots"><span>Final step</span></li>
                  </ul>
                </div>
              </div>

            </form>

          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- end of table -->

<script>
$(document).on("click",'#checker',function(){

});

</script> 

